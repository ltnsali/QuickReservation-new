{
  "expo": {
    "name": "QuickReservation",
    "slug": "QuickReservation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "qreserv",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qreserv.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.qreserv.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "config": {
        "google": {
          "clientId": "57102764070-q106sapm1qn0rh33qrgqlpjnha9hpu0r.apps.googleusercontent.com"
        }
      },
      "output": "single",
      "build": {
        "babel": {
          "include": ["@expo/vector-icons"]
        }
      },
      "headers": {
        "Content-Security-Policy": "default-src 'self'; script-src 'self' 'nonce-quickreservation' https://accounts.google.com https://*.googleapis.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https:; connect-src 'self' https://*.googleapis.com https://*.firebaseio.com wss://*.firebaseio.com; frame-src 'self' https://accounts.google.com"
      }
    },
    "plugins": ["expo-router"]
  }
}
